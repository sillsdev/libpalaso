<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <!--- ATTENTION: The following dllmaps are a mono thing, not parsable by VS on Windows.
        They will cause an error any time something in VS
        tries to edit this file, e.g. making a new settings entry.
        As a workaround, comment them out,
        then make your change, then add them back. -->
    <dllmap dll="libgdk-x11-2.0" target="libgdk-x11-2.0.so.0"/>
    <dllmap dll="libgtk-x11-2.0" target="libgtk-x11-2.0.so.0"/>
