    <ProjectReference Include="..\SIL.Windows.Forms.WritingSystems\SIL.Windows.Forms.WritingSystems.csproj" />
    <ProjectReference Include="..\SIL.Windows.Forms\SIL.Windows.Forms.csproj" />
